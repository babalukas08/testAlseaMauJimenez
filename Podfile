target 'TestAlsea' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!

  workspace 'TestAlsea'

  # Pods for TestAlsea
  source 'https://github.com/CocoaPods/Specs.git'

  def basic_pods
    pod 'Alamofire'
    pod 'ObjectMapper'
    pod 'AlamofireObjectMapper'
    pod 'AlamofireImage'
    pod 'NibDesignable'
end

target 'TestAlsea' do
    project 'TestAlsea.xcodeproj'
    basic_pods
end

end
